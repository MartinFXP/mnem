citHeader("To cite bnem in publications use:")

citEntry(entry = "Article",
  title        = "Single cell network analysis with a mixture of Nested Effects Models.",
  author       = "Martin Pirkl, Niko Beerenwinkel",
  journal      = "bioRxiv",
  year         = "2018",
  number       = "258202",
  url          = "https://doi.org/10.1101/258202",

  textVersion  =
  paste("Martin Pirkl, Niko Beerenwinkel
Single cell network analysis with a mixture of Nested Effects Models
bioRxiv 258202; doi: https://doi.org/10.1101/258202")
)
